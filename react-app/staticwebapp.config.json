{
  "routes": [
    {
      "route": "/api/products/*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/.auth/login/aad",
      "statusCode": 404
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{css,scss,js,png,gif,ico,jpg,svg}"
    ]
  },
  "networking": {
    "allowedIpRanges": [
      "192.168.1.70/32"
    ]
  }
}